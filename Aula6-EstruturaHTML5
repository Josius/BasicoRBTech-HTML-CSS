<!-- ESTRUTURA HTML5 -->

<!doctype html>
<html>
	<head>
		<title>Estrutura em HTML5</title>
		<meta charset="utf-8" />

	</head>
	
	<body>
		<header>
			Topo do site + menus de navegação
			<nav>Menu de navegação (no header)</nav>
		</header>
		<nav>
			Menu de navegação (fora do header)
		</nav>
		<!-- PARTE CENTRAL -->
		<section>
			<header><h1>Título da seção (tag header dentro da tag section)</h1></header>
			<p>Conteúdo da Seção</p>
			<footer>Rodapé da seção(tag footer dentro da tag section)</footer>
		</section>
		<article>
			<header><h1>Título do artigo (tag header dentro da tag article)</h1></header>
			<p>Conteúdo da Artigo</p>
			<footer>Rodapé do Artigo(tag footer dentro da tag article)</footer>
		</article>
		<aside>
			Barra lateral (sidebar)
		</aside>
		<footer>
			Rodapé do site
		</footer>
	</body>
</html>
